 },\\r\\n      setMar: function(){\\r\\n        marker.draggable(true);\\r\\n      },\\r\\n      range: function ( low, high ) {\\r\\n          var half = high - (high | 0);\\r\\n          if (half >= .5){\\r\\n              MAIN.set(\\\'halfstar\\\', true);\\r\\n          }\\r\\n          else{\\r\\n              MAIN.set(\\\'halfstar\\\', false);\\r\\n          }\\r\\n          high = high | 0;\\r\\n          var range = [];\\r\\n          for ( i = low; i <= high; i += 1 ) {\\r\\n            range.push( i );\\r\\n          }\\r\\n          return range;\\r\\n        }\\r\\n\\r\\n\\r\\n    }\\r\\n  });\\r\\n\\r\\n    function load_reviews () {\\r\\n       $.ajax("\', escape=False)\nresponse.write(URL(\'default\', \'load_reviews\', args= [post.id]))\nresponse.write(\'",\\r\\n\\r\\n          {\\r\\n            method: \\\'POST\\\',\\r\\n            success: function (data) {\\r\\n              MAIN.set(\\\'review_dict\\\', data[\\\'review_dict\\\']);\\r\\n              MAIN.set(\\\'loading\\\', false);\\r\\n            }\\r\\n          }\\r\\n        );\\r\\n    }\\r\\n\\r\\n\\r\\n\\r\\n\\r\\n    MAIN.on("Del", function(e, rev_id, post){\\r\\n        MAIN.set(\\\'reviewed\\\', false);\\r\\n        $.ajax("\', escape=False)\nresponse.write(URL(\'default\', \'delete_review\', user_signature=True))\nresponse.write(\'",\\r\\n              {\\r\\n                data: {\\r\\n                  rev_id: rev_id,\\r\\n                  post: post\\r\\n                },\\r\\n                method: \\\'POST\\\',\\r\\n                success: function() {\\r\\n//                    alert(\\\'deleted\\\');\\r\\n                },\\r\\n                error: function() {\\r\\n                }\\r\\n              }\\r\\n        );\\r\\n        load_reviews();\\r\\n    });\\r\\n//\\r\\n//    MAIN.on("Edit", function(e, rev_id, post, user_id){\\r\\n//        alerter(rev_id, post,user_id);\\r\\n//    });\\r\\n\\r\\n    function load_Position(){\\r\\n          $.ajax("\', escape=Fals